<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <ImageView
        android:id="@+id/jrope"
        android:layout_width="400dp"
        android:layout_height="110dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@android:color/black"
        app:srcCompat="@drawable/jrope" />

    <Button
        android:id="@+id/elip1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jrope"
        android:layout_alignParentStart="true"
        android:layout_gravity="center"
        android:layout_marginTop="36dp"
        android:background="#00ff00"
        android:text="Front 2 Back"
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jrope"
        android:layout_alignParentBottom="false"
        android:layout_marginStart="21dp"
        android:layout_marginTop="36dp"
        android:layout_toEndOf="@+id/elip1"
        android:background="#00ff00"
        android:text="Incline Buster" />

    <Button
        android:id="@+id/elip3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jrope"
        android:layout_alignParentBottom="false"
        android:layout_marginStart="21dp"
        android:layout_marginTop="36dp"
        android:layout_toEndOf="@+id/elip2"
        android:background="#00ff00"
        android:text="20 min HIIT
" />

    <Button
        android:id="@+id/elip4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/elip2"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:layout_marginStart="0dp"

        android:layout_marginTop="62dp"
        android:layout_toEndOf="@+id/elip1"
        android:background="#ffff00"
        android:text="Burn and Build"
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/elip2"
        android:layout_alignParentStart="true"
        android:layout_gravity="center"
        android:layout_marginStart="146dp"
        android:layout_marginTop="62dp"
        android:layout_toEndOf="@+id/elip4"
        android:background="#ffff00"
        android:text="30min Sprint Build
"
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/elip2"
        android:layout_alignParentStart="true"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginTop="138dp"
        android:layout_toEndOf="@+id/elip5"
        android:background="#ffff00"
        android:text="Mix it Up"
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="237dp"
        android:background="@android:color/holo_red_light"
        android:text="Sprints + Endurance "
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginStart="195dp"
        android:layout_marginBottom="233dp"
        android:background="@android:color/holo_red_light"
        android:text="Backward Running"
        android:textStyle="bold" />

    <Button
        android:id="@+id/elip9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="127dp"
        android:background="@android:color/holo_red_light"
        android:text="Hill Sprints "
        android:textStyle="bold" />

</RelativeLayout>